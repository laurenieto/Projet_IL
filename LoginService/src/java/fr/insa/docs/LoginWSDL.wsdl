<?xml version="1.0" encoding="UTF-8"?>
<definitions name="LoginWSDL" targetNamespace="http://j2ee.netbeans.org/wsdl/LoginService/LoginWSDL"
    xmlns="http://schemas.xmlsoap.org/wsdl/"
    xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://j2ee.netbeans.org/wsdl/LoginService/LoginWSDL" xmlns:ns="http://xml.netbeans.org/schema/LoginRequest" xmlns:ns0="http://xml.netbeans.org/schema/LoginReply" xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype">
    <types>
        <xsd:schema targetNamespace="http://j2ee.netbeans.org/wsdl/LoginService/LoginWSDL">
            <xsd:import namespace="http://xml.netbeans.org/schema/LoginReply" schemaLocation="LoginReply.xsd"/>
            <xsd:import namespace="http://xml.netbeans.org/schema/LoginRequest" schemaLocation="LoginRequest.xsd"/>
        </xsd:schema>
    </types>
    <message name="isInDBOperationRequest">
        <part name="LoginInfo" element="ns:LoginRequest"/>
    </message>
    <message name="isInDBOperationResponse">
        <part name="isInDBReply" element="ns0:LoginReply"/>
    </message>
    <portType name="LoginWSDLPortType">
        <operation name="isInDBOperation">
            <input name="input1" message="tns:isInDBOperationRequest"/>
            <output name="output1" message="tns:isInDBOperationResponse"/>
        </operation>
    </portType>
    <plnk:partnerLinkType name="LoginWSDL">
        <!-- A partner link type is automatically generated when a new port type is added. Partner link types are used by BPEL processes. 
In a BPEL process, a partner link represents the interaction between the BPEL process and a partner service. Each partner link is associated with a partner link type.
A partner link type characterizes the conversational relationship between two services. The partner link type can have one or two roles.-->
        <plnk:role name="LoginWSDLPortTypeRole" portType="tns:LoginWSDLPortType"/>
    </plnk:partnerLinkType>
</definitions>

